{
	"suggest.maxPreviewWidth": 999,
	"suggest.snippetIndicator": "⭐︎",
	"snippets.userSnippetsDirectory": "~/.config/nvim/UltiSnips",
	"snippets.editSnippetsCommand": "split",
	"highlight.colors.enable": true,
	"highlight.document.enable": true,
	"emmet.includeLanguages": {
		"vue-html": "html",
		"javascript": "javascriptreact",
		"gfimarkdown": "html",
		"markdown": "html"
	},
	"template.license": "GPL",
	"template.templatesDirectory": "~/.config/nvim/vim-template",
	"leetcode.language": "c",
	"list": {
		"source": {
			"bibtex": {
				"files": [
					"*.bib"
				],
				"citation": {
					"before": "\\cite{",
					"after": "}"
				}
			}
		}
	},
	"list.insertMappings": {
		"<C-l>": "prompt:removetail",
		"<C-y>": "prompt:paste",
		"<C-f>": "prompt:right",
		"<C-b>": "prompt:left",
		"<C-g>": "feedkeys:<PageUp>",
		"<C-v>": "feedkeys:<PageDown>",
		"<C-s>": "do:toggle",
		"<C-x>": "do:selectall",
		"<C-q>": "do:switch"
	},
	"list.normalMappings": {
		"u": "feedkeys:<PageUp>",
		"d": "feedkeys:<PageDown>",
		"t": "feedkeys:t",
		"<Space>": "feedkeys:<Space>",
		"?": "feedkeys:?",
		"o": "do:toggle",
		"x": "action:delete"
	},
	"explorer.icon.enableNerdfont": true,
	"explorer.icon.enableVimDevicons": true,
	"explorer.keyMappingMode": "none",
	"explorer.keyMappings": {
		"k": "nodePrev",
		"j": "nodeNext",
		"K": "gotoParent",
		"zk": "expandablePrev",
		"zj": "expandableNext",
		"g!": "actionMenu",
		"s": ["toggleSelection", "nodeNext"],
		"S": ["toggleSelection", "nodePrev"],
		"zo": "expand",
		"zc": "collapse",
		"za": "expandOrCollapse",
		"zO": "expandRecursive",
		"zC": "collapseRecursive",
		"o": ["expandable?", "expandOrCollapse", "open"],
		"O": "open:tab",
		"i": "open:split",
		"I": "open:vsplit",
		"go": ["expandable?", "expandOrCollapse", "open", "quit"],
		"gO": ["open:tab", "quit"],
		"gi": ["open:split", "quit"],
		"gI": ["open:vsplit", "quit"],
		"c": "copyFile",
		"C": "copyFilename",
		"<C-c>": "copyFilepath",
		"x": "cutFile",
		"X": "delete",
		"<c-x>": "deleteForever",
		"p": "pasteFile",
		"P": "preview:labeling",
		".": "toggleHidden",
		"a": "addFile",
		"A": "addDirectory",
		"r": "rename",
		"R": "refresh",
		"<F1>": "help",
		"ZQ": "quit",
		"<esc>": "esc",
		"gx": "systemExecute",
		"gd": "listDrive",
		"gs": "search",
		"gS": "searchRecursive",
		"gf": "gotoSource:file",
		"gF": "gotoSource:buffer",
		"[[": "sourcePrev",
			"]]": "sourceNext",
			"[d": "diagnosticPrev",
				"]d": "diagnosticNext",
				"[g": "gitPrev",
					"]g": "gitNext",
					"<": "gitStage",
					">": "gitUnstage"
	},
	"snippets.ultisnips.directories": [
		"UltiSnips",
		"gosnippets/UltiSnips",
	],
	"git.enableGutters": true
}

